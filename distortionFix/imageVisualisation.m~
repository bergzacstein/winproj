cell=imread('images/bat.tif');
% imshow(cell);

se = makeSE('square', 2);

ero=erosion(cell,se);
dil=dilation(cell,se);
open=opening(cell,se);
close=closing(cell,se);

imshow(cell);
figure(2);
imshow(dil);

tophat = open-cell